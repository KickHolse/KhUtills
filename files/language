messages:
  without-permission: '&cVocê não tem permissão para isto.'
  player-offline: '&cEste jogador não esta online no momento.'
  clear:
    your-inventory: '&aSeu inventário foi limpo'
    other-inventory:  '&aVocê limpou o inventário de &2{player}'
  invsee:
    usage: '&cUtilize /invsee <jogador>'
    success: '&aVocê abriu o inventário de &2{player}'
  ping:
    yourself: '&aVocê está com &2{ping}ms &7({state}&7)'
    other-player: '&2{player} &aestá com &2{ping}ms &7({state}&7)'
    replaces:
      good: '&a&lBOM'
      medium: '&e&lMÉDIO'
      bad: '&c&lRUIM'
  gamemode:
    usage: '&cUtilize /gamemode <modo> <jogador>'
    yourself: '&aSeu modo de jogo foi alterado para &2{gamemode}'
    other-player: '&aO modo de jogo de &2{player}&a foi alterado para &2{gamemode}'
    replaces:
      survival: 'Sobrevivência'
      creative: 'Criativo'
      adventure: 'Aventura'
      spectator: 'Espectador'
  warn:
    usage: '&cUtilize /warn <mensagem>'
    sound: 'LEVEL_UP'
    broadcast:
      enable: true
      format: '\n &6{player}&a:&f {warn}\n '
    action-bar:
      enable: false
      format: '&6{player}&a:&f {warn}'
    title:
      enable: false
      title: '&c&lAVISO'
      subtitle: '&7Olhe o chat!'
